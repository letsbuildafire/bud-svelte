{
  "name": "bud-svelte-loader",
  "version": "0.0.1",
  "description": "svelte support for @roots/bud projects",
  "engines": {
    "node": ">=16"
  },
  "contributors": [
    {
      "email": "dakoda@letsbuildafire.com",
      "name": "Dakoda",
      "url": "https://github.com/letsbuildafire"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/letsbuildafire/bud-svelte"
  },
  "bugs": "https://github.com/letsbuildafire/bud-svelte/issues",
  "keywords": [
    "bud",
    "extension",
    "svelte"
