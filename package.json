    "@roots/bud-build": "^6.16.1",
    "@roots/bud-extensions": "^6.16.1",
    "@roots/bud-support": "^6.16.1",
    "@roots/bud-api": "^6.16.1",
    "tslib": "2.6.2"
  },
  "dependencies": {
    "svelte": "^4.2.8",
    "svelte-loader": "^3.1.2"
  },
  "bud": {
    "version": ">=6.16.0",
    "type": "extension"
  }
